{
  "name": "@mentimeter/play-lambda",
  "version": "1.0.18",
  "description": "Runs playwright tests in lambda functions",
  "bin": "./cli.js",
  "sideEffects": false,
  "scripts": {
    "test": "run -T jest",
    "postinstall": "node patch-playwright-export.js",
    "publish": "rm -rf dist && yarn tsc && yarn npm publish --tag latest"
  },
  "files": [
    "cli.js",
    "serverless.yml",
    "patch-playwright-export.js",
    "/dist",
    "/runner"
  ],
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.48.0",
    "@aws-sdk/client-s3": "^3.48.0",
    "@playwright/test": "1.21.1",
    "argparse": "^2.0.1",
    "dotenv": "^16.0.0",
    "esbuild-register": "^3.1.2",
    "esbuild": "^0.14.14",
    "glob": "^7.2.0",
    "mime-types": "^2.1.34"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.10",
    "@types/jest": "^27",
    "@types/node": "^17.0.31",
    "jest": "^28",
    "ts-jest": "^28.0.0",
    "typescript": "^4.6.2"
  }
}
